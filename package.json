{
  "name": "j-html-design-editor",
  "version": "1.0.62",
  "description": "design html 设计图片编辑库",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "node test/dev",
    "build": "gulp && node build.js",
    "test": "vitest",
    "update": "npx taze major  -w",
    "release": "npx bumpp --commit --tag --push",
    "api:json": "npx api-extractor run",
    "api:md": "npx api-documenter markdown -i ./docs/.api -o ./docs/api && node ./build/md.js",
    "api": "npm run api:json && npm run api:md",
    "push": "npm publish --registry=https://registry.npmjs.org",
    "docs:dev": "npm run api && vitepress dev",
    "docs:build": "npm run api && vitepress build",
    "docs:preview": "npm run api && vitepress preview"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-function-bind": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-stage-0": "^7.8.3",
    "@microsoft/api-documenter": "^7.23.20",
    "@microsoft/api-extractor": "^7.39.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "happy-dom": "^13.3.5",
    "html-to-image": "^1.11.11",
    "j-figma2html": "^1.0.28",
    "request": "^2.88.2",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsify": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vinyl-source-stream": "^2.0.0",
    "vitepress": "^1.0.0-rc.40",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "j-css-filters": "^1.0.3",
    "j-design-util": "^1.0.37",
    "j-eventemitter": "^1.0.1"
  },
  "author": "fefeding",
  "license": "MIT"
}
